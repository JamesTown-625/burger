<h1>Hamburgers!</h1>

<h3>Eat the Burgers!</h3>

<ul>
  {{#each burgers}}
    <li>
       <p>ID: {{this.id}}</p>
       <p>BURGER: {{this.burger_name}}</p>
    </li>
  {{/each}}
</ul>

<form action="/" method="POST">
  <textarea type="text" name="burger"></textarea>

  <button type="submit">Create</button>
</form>
